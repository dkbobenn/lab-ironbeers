
{{#each responseFromAPI}} 
</div>
<div class="col-md-4">
  <div class="card">
    <img class="card-img-top" src="{{this.image_url}}" alt="{{this.name}}"/>
    <div class="card-body">
      <h5 class="card-title">
        {{this.name}}
      </h5>
      <p class="card-text">
        {{this.description}}
      </p>
      <p class="card-text">
        {{this.tagline}}
      </p>
      <h5>Food Pairing</h5>
      <p>{{this.food_pairing }}</p>
      <h5>Brewers Tips</h5>
      <p>{{this.brewers_tips}}</p>
    </div>
  </div>
</div>
{{/each}}

